<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="add-column-measure" author="a.khusainov.stc@innopolis.university">
        <addColumn tableName="measure">
            <column name="store_measure"
                    type="varchar(255)">
            </column>
        </addColumn>
        <addColumn tableName="measure">
            <column name="name"
                    type="varchar(255)">
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>
